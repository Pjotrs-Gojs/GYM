{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.GYM.GYM",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"GYM.xsodata": {
				"uri": "/BB/com/GYM/Service/GYM.xsodata/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.GYM.GYM.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.GYM.GYM.i18n.i18n",
					"bundleLocale" : "en-US"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "GYM.xsodata",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.GYM.GYM.view",
				"controlId": "layout",
				"controlAggregation": "beginColumnPages"
			},
			"routes": [{
				"pattern": "",
				"name": "Master",
				"target": "Master"
			}, {
				"pattern": "Trainer/{TrainerID}",
				"name": "Details",
				"target": ["Master", "Details"]
			}],
			"targets": {
				"Master": {
					"viewName": "Master",
					"viewLevel": 0,
					"viewId": "Master"
				},
				"Details": {
					"viewName": "Details",
					"viewId": "Details",
					"viewLevel": 1,
					"controlAggregation": "midColumnPages"
				}
			}
		}
	}
}